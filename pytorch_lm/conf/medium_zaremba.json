{
	"model": {
		"class": "MediumZarembaModel"
	},
	"optimizer": {
		"class": "torch.optim.SGD",
		"args": [1.0]
	},
	"initializer": {
		"function": "uniform",
		"kwargs": {"a": -0.05, "b": 0.05}
	},
	"lr_scheduler": {
		"class": "ZarembaScheduleLR",
		"kwargs": {
			"lr_decay": 0.83,
			"decay_delay": 6
		}
	},
	"num_epochs": 39,
	"batch_size": 20,
	"num_steps": 35,
	"grad_clip": 5
}
