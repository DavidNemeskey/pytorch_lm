{
    "model": {
        "class": "MerityModel",
        "kwargs": {
            "hidden_size": 1150,
	    "embedding_size": 400,
            "num_layers": 3,
            "dropout": "0.3s",
	    "rnn": {
		"class": "Lstm",
		"input_dropout": "0.4s",
		"layer_dropout": "0.3s",
		"cell_data": {
		    "class": "MerityLstmCell",
		    "kwargs": {
			"dropconnect": "0.5s"
		    }
		}
	    },
            "embedding_dropout": "0.1s",
            "output_dropout": "0.4s",
	    "alpha": 2,
	    "beta": 1
        }
    },
    "optimizer": {
        "class": "torch.optim.SGD",
        "args": [1.0]
    },
    "initializer": {
        "function": "uniform",
        "kwargs": {"a": -0.1, "b": 0.1}
    },
    "lr_scheduler": {
        "class": "ZarembaScheduleLR",
        "kwargs": {
            "lr_decay": 0.83,
            "decay_delay": 6
        }
    },
    "num_epochs": 750,
    "batch_size": 40,
    "num_steps": {
	"class": "RandomNumSteps",
	"kwargs": {
	    "num_steps": 70,
	    "p": 0.95,
	    "s": 5
	}
    },
    "grad_clip": 0.25
}
